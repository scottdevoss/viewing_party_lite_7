<h1>Top Movies</h1>

<%= button_to "Discover Page", "/users/#{params[:id]}/discover", method: :get %>

<% @movies.each do |movie| %>
  <li><%= link_to "#{movie[:title]}", "/users/#{params[:id]}/movies/#{movie[:id]}", method: :get %></li>
  <li><%= "Vote Average: #{movie[:vote_average]}" %></li>
<% end %>